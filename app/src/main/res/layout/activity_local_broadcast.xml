<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_local_broadcast"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rainy.webviewproject.LocalBroadcastActivity">
    <TextView
        android:onClick="click"
        android:id="@+id/text"
        android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/activity_local_broadcast"
        app:layout_constraintLeft_toLeftOf="@+id/activity_local_broadcast"
        app:layout_constraintRight_toRightOf="@+id/activity_local_broadcast"
        app:layout_constraintTop_toTopOf="@+id/activity_local_broadcast"/>
</android.support.constraint.ConstraintLayout>
